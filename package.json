{
  "name": "ts-logger",
  "version": "0.1.5",
  "description": "An experimental logger library for NodeJS written in TypeScript",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run typings && npm run bundle",
    "typings": "node_modules/typings/dist/bin.js install",
    "build": "rm -rf dist && node_modules/typescript/bin/tsc -p tsconfig.build.json --declaration",
    "bundle": "npm run build && typings bundle -o dist/types.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cubodehelio/ts-logger.git"
  },
  "author": "Juan M. Ca√±abate [@cubodehelio]",
  "license": "ISC",
  "homepage": "https://github.com/cubodehelio/ts-logger#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "lodash.iserror": "^3.1.1",
    "node-uuid": "^1.4.7",
    "on-finished": "^2.3.0"
  },
  "devDependencies": {
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.5"
  }
}
